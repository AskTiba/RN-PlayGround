{
  "expo": {
    "name": "ModX",
    "slug": "RN-PlayGround",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/brain.png",
    "scheme": "modx",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/3dRender.png",
      "resizeMode": "center",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.asktiba.RNPlayGround"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.asktiba.RNPlayGround"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cbaa4844-ea91-4550-9110-3dbc35c509a4"
      }
    },
    "owner": "asktiba"
  }
}
